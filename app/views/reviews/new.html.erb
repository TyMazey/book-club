<h1>New Review</h1>
<%= form_for @review, url: book_reviews_path(@book) do |f|%>
<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :description %>
<%= f.text_area :description %>

<%= f.label :rating %>
<%= f.select :rating, 1..5 %>

<%= f.label :user %>
<%= f.text_field :user %>

<%= f.submit :Save %>
<% end %>
